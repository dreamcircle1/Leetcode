// 递归解法，超时
class Solution {
public:
    int climbStairs(int n) {
        if(n==1)    return 1;
        else if(n==2)    return 2;
        else return climbStairs(n-1) + climbStairs(n-2);
    }
};

//自底向上求解
class Solution {
public:
    int climbStairs(int n) {
        if(n==1)    return 1;
        else if(n==2)   return 2;
        int result;
        int pre=2;
        int prePre=1;
        for(int i=3;i<=n;++i){
            result=pre + prePre;
            prePre=pre;
            pre=result;
        }
        return result;
    }
};

